{
  "name": "shoppers-ui",
  "version": "1.0.0",
  "description": "shoppers-ui",
  "main": "index.js",
  "engine": {
    "node": ">=16.0.0"
  },
  "scripts": {
    "build": "babel ./server --out-dir ./build",
    "start": "node ./build/index.js",
    "node": "nodemon server/index.js",
    "node-exp": "nodemon --experimental-json-modules --exec babel-node server/index.js",
    "node-debug": "set DEBUG=* & npm run node",
    "serve": "ng serve --proxy-config proxy.config.json",
    "watch": "ng build --watch --configuration development",
    "test": "ng test"
  },
  "author": "ibssoln",
  "license": "ISC",
  "dependencies": {
    "@angular/animations": "^15.1.0",
    "@angular/common": "^15.1.0",
    "@angular/compiler": "^15.1.0",
    "@angular/core": "^15.1.0",
    "@angular/forms": "^15.1.0",
    "@angular/platform-browser": "^15.1.0",
    "@angular/platform-browser-dynamic": "^15.1.0",
    "@angular/router": "^15.1.0",
    "@clr/angular": "^13.18.2",
    "@clr/core": "^4.0.15",
    "@clr/icons": "^13.0.2",
    "@clr/ui": "^13.18.2",
    "axios": "^0.21.4",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "ngx-quill": "^20.0.0",
    "nodemon": "^3.0.1",
    "path": "^0.12.7",
    "quill": "^1.3.7",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "winston": "^3.8.2",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.10",
    "@angular/cli": "^15.2.10",
    "@angular/compiler-cli": "^15.1.0",
    "@types/jasmine": "~4.3.0",
    "jasmine-core": "~4.5.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.9.4",
    "@types/quill": "1.3"
  }
}
